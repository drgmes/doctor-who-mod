{
  "schemaVersion": 1,
  "id": "${modId}",
  "version": "${modVersion}",
  "name": "${modName}",
  "description": "${modDescription}",
  "authors": ["${modAuthors}"],
  "contact": {
    "homepage": "${modHomepage}",
    "sources": "${modSource}"
  },
  "license": "${modLicense}",
  "icon": "assets/${modId}/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.drgmes.dwm.fabric.DWMFabric"
    ],
    "client": [
      "net.drgmes.dwm.fabric.DWMFabric"
    ],
    "server": [
      "net.drgmes.dwm.fabric.DWMFabric"
    ],
    "fabric-datagen": [
      "net.drgmes.dwm.fabric.datagen.DataGenerators"
    ],
    "modmenu": [
      "net.drgmes.dwm.compat.ModMenu"
    ]
  },
  "mixins": [
    "dwm.mixins.json",
    "dwm-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraftVersion}",
    "architectury": ">=${architecturyVersion}",
    "forgeconfigapiport": ">=${forgeConfigApiPortVersion}"
  }
}
